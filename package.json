{
  "name": "todozen",
  "version": "3.4.0",
  "description": "A simple GNOME todo list extension (forked and enhanced as TodoZen)",
  "main": "extension.js",
  "repository": "git@github.com:Irtesaam/todozen.git",
  "type": "module",
  "author": "Irtesaam <atfimdirtesaam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:strict": "eslint src/ --rule '@typescript-eslint/no-explicit-any: warn'",
    "check": "tsc --noEmit && eslint src/",
    "check:all": "tsc --noEmit && eslint src/ && vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.14.0",
    "eslint-plugin-jsdoc": "^62.0.0",
    "globals": "^17.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@girs/gjs": "^4.0.0-beta.16",
    "@girs/gnome-shell": "^46.0.0"
  }
}
